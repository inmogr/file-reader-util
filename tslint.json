{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "prefer-for-of": false,
        "interface-name": false,
        "no-namespace": false,
        "max-line-length": [
            true,
            140
        ],
        "prefer-const": true,
        "quotemark": [
            true,
            "double"
        ]
    }
}
